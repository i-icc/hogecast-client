{
  "hosting": {
    "source": ".",
    "site": "ai-cast",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "asia-east1"
    },
    "rewrites": [
      {
        "source": "/_next/image",
        "destination": "/_next/image"
      },
      {
        "source": "/_next/static/*",
        "destination": "/_next/static/*"
      }
    ],
    "dynamicRoutes": [
      {
        "page": "/podcasts/[radioKey]",
        "regex": "^/podcasts/([^/]+?)(?:/)?$",
        "routeKeys": {
          "radioKey": "radioKey"
        },
        "namedRegex": "^/podcasts/(?<radioKey>[^/]+?)(?:/)?$"
      }
    ]
  }
}